{
  "version": 2,
  "functions": {
    "server/index.js": {
      "includeFiles": "server/**"
    }
  },
  "rewrites": [
    {
      "source": "/(register|login|role|peserta|admin|logout)(.*)",
      "destination": "/server/index.js"
    },
    {
      "source": "/(.*)",
      "destination": "/client/dist/$1"
    }
  ]
}